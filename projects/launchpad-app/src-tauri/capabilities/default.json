{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Launchpad default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "fs:default",
    "fs:allow-read-dir",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-exists",
    "fs:allow-mkdir",
    "fs:allow-rename",
    "fs:allow-remove",
    "dialog:default",
    "shell:allow-open"
  ]
}
